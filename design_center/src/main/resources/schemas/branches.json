{
  "title": "List of branches of the project",
  "type": "array",
  "items":{
    "title": "Branch",
    "type": "object",
    "properties": {
      "name": {
        "type": "string",
        "maxLength": 128,
        "pattern": "^[a-zA-Z0-9_]*$"
