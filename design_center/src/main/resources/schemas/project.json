{
  "title": "Project",
  "type": "object",
  "properties": {
    "organizationId": {
      "title": "OrganizationId",
      "maxLength": 2048,
      "type": "string"
    },
    "id": {
      "title": "Id",
      "type": "string",
      "maxLength": 36,
      "minLength": 36,
      "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$"
    },
    "name": {
      "title": "Project name",
      "minLength": 1,
      "maxLength": 128,
      "type": "string",
      "pattern": "[a-zA-Z0-9_\\-\\s]*"
    },
    "description": {
      "title": "Project description",
      "maxLength": 1024,
      "type": "string"
    },
    "type": {
      "title": "Project type",
      "maxLength": 16,
      "type": "string",
      "pattern": "[a-zA-Z0-9_\\-]*"
    },
    "product": {
      "title": "Project product",
      "maxLength": 16,
      "type": "string",
      "pattern": "[a-zA-Z0-9_\\-]*"
    },
    "vcsType": {
      "title": "Vcs type",
      "enum": ["git"],
      "default": "git"
    },
    "externalUri": {
      "description": "externalUri can be used to clone the repository with the vcs command line"
    },
    "createdBy": {
      "description": "User that created the project"
    },
    "createdDate": {
      "description": "Project creation date"
    }
  }
}