{
  "title": "BranchFileCreate",
  "type": "object",
  "properties": {
    "path": {
      "title": "Path of the file. Cannot contains spaces.",
      "maxLength": 128,
      "type": "string",
      "example": "file-path",
      "pattern": "[/a-zA-Z0-9_\\-\\.]*"
    },
    "commitMessage": {
      "title": "Message of the commit that will be generated when uploading the file.",
      "maxLength": 64,
      "type": "string",
      "example": "This is the commit message"
    },
    "content": {
      "title": "Content of the file encoded using base64",
      "type": "string",
      "example": "VGhpcyBpcyB0aGUgZmlsZSBjb250ZW50Cg=="
    }

  },
  "required": ["path", "commitMessage"]
}