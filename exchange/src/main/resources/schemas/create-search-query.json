{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "minLength": 1,
      "maxLength": 64
    },
    "types": {
      "type": "array",
      "minItems": 0,
      "maxItems": 16,
      "items": {
        "type": "string",
        "minLength": 1,
        "maxLength": 64
      }
    },
    "search": {
      "type": "string",
      "minLength": 0,
      "maxLength": 1024
    }
  },
  "required": [
    "name",
    "search"
  ]
}
