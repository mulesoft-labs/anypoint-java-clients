{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "displayName": {
      "type": "string",
      "maxLength": 512,
      "minLength": 1
    },
    "dataType": {
      "type": "string",
      "maxLength": 128,
      "enum": ["enum", "number", "date", "text", "number-list", "text-list"]
    },
    "assetTypeRestrictions": {
      "type": "array",
      "maxItems": 100,
      "items": {
        "type": "string",
        "maxLength": 100,
        "minLength": 1
      }
    },
    "acceptedValues": {
      "type": "array",
      "maxItems": 100,
      "items": {
        "type": "string",
        "maxLength": 100,
        "minLength": 1
      }
    }
  }
}
