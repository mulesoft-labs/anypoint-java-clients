#%RAML 1.0 DataType
type: object

properties:
  status:
    type: !include status.raml
    description: Asset publication status
  assetType:
    type: string
    description: Inferred asset type, can be null in early steps.
  steps:
    type: array
    items:
      type: !include publicationStep.raml
    description: Asset publication steps
